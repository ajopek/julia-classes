   "source": [
    "function harmonic_mean(x...)\n",
    "        N = length(x)\n",
    "        ex = :($N)\n",
    "        ex2 = :(0)\n",
    "        for i=1:N\n",
    "            ex2 = :((1/x[$i])+$ex2)\n",
    "        end\n",
    "        return :($ex/$ex2)\n",
    "end    "
   ]
